{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: ['github>nicksp/renovate-config:default.json5'],
  automerge: true,
  packageRules: [
    {
      // Disable digest updates for self-referencing workflows/actions
      matchDatasources: ['github-actions'],
      matchPackageNames: ['nicksp/workflows/.github/actions/setup-repo'],
      matchUpdateTypes: ['digest'],
      enabled: false,
    },
  ],
}
